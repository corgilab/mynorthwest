{
  "name": "mynorthwest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.8.2",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.53.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-map-gl": "^4.0.10",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "npm run dev:webpack",
    "build": "npm run build:webpack",
    "dev:webpack": "cross-env NODE_ENV=dev webpack-dev-server --open --mode development",
    "build:webpack": "cross-env NODE_ENV=prod webpack --mode production && npm run svg:compress",
    "eslint": "eslint ./src/",
    "svg:compress": "svgo -f ./resources/images",
    "prettier": "prettier --write \"./src/**/*.js\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "pre-commit": [
    "prettier",
    "eslint"
  ]
}
